{
  "name": "newsims",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pins": "npx pod-install -ios",
    "pins-M1": "cd ios && arch -x86_64 pod install && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "pod-update": "cd ios && arch -x86_64 pod repo update",
    "android-release": "react-native run-android --variant=release",
    "apk-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab-release": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "signingReport": "cd android && ./gradlew signingReport",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "code-push": "appcenter codepush release-react -a Chickers/IMS-ECOMMERCE-IOS -d Production && appcenter codepush release-react -a Chickers/IMS-ECOMMERCE-ANDROID -d Production",
    "code-push-clear": "appcenter codepush deployment clear -a Chickers/IMS-ECOMMERCE-IOS Production && appcenter codepush deployment clear -a Chickers/IMS-ECOMMERCE-ANDROID Production"
  },
